{
  "name": "@cytracom/softphone",
  "version": "1.0.0",
  "description": "Cytracom Softphone Feature",
  "author": "jwhenry3",
  "license": "MIT",
  "repository": "cytracom/softphone",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "fix": "prettier --write . && eslint . --fix",
    "ci": "yarn install --frozen-lockfile && yarn build && cd app && yarn install --frozen-lockfile",
    "build": "microbundle-crl --no-compress --format modern,cjs && cp package.dist.json dist/package.json",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "husky install",
    "test": "yarn test:unit && yarn test:lint",
    "test:build": "yarn build",
    "test:lint": "eslint .",
    "test:unit": "DISABLE_NEW_JSX_TRANSFORM=true cross-env CI=1 react-scripts test",
    "test:watch": "DISABLE_NEW_JSX_TRANSFORM=true react-scripts test",
    "storybook": "start-storybook -p 6006 -s public",
    "deploy": "./deploy.sh",
    "build-storybook": "build-storybook -s public",
    "deploy-storybook": "storybook-to-ghpages --existing-output-dir=storybook-static --ci --host-token-env-variable=GITHUB_TOKEN",
    "postinstall": "touch ./node_modules/.metadata_never_index"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "peerDependencies": {
    "@cytracom/telos": "^1.0.8",
    "@emotion/react": "^11.1.5",
